{
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "vhttps://finance-portfolio-back.onrender.com/api/$1"
    },
    {
      "source": "/oauth2/(.*)",
      "destination": "https://finance-portfolio-back.onrender.com/oauth2/$1"
    },
    {
      "source": "/login/oauth2/(.*)",
      "destination": "https://finance-portfolio-back.onrender.com/oauth2/$1"
    },
    {
      "source": "/((?!_astro|favicon.svg|assets/).*)",
      "destination": "/index.html"
    }
  ]
}
