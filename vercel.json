{
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "https://d123abc456def.cloudfront.net/api/$1"
    },
    {
      "source": "/oauth2/(.*)",
      "destination": "https://d123abc456def.cloudfront.net/oauth2/$1"
    },
    {
      "source": "/login/oauth2/(.*)",
      "destination": "https://d123abc456def.cloudfront.net/login/oauth2/$1"
    }
  ]
}